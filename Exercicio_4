//Preencher um vetor com 5 números e à medida que for digitado o número, calcular o cubo e mostrar em outro vetor. Mostrar os dois vetores.

#include <stdio.h>

int main() {
   
   int numeros[5], cubos[5];
   
   for (int i = 0; i < 5; i++){
       printf("Digite o numero %d: ", i+1);
       scanf("%d", &numeros[i]);
       cubos[i] = numeros[i] * numeros[i] * numeros[i];
   }
   
   printf("Numero:\n Cubo:\n");
   for (int i = 0; i < 5; i++){
       printf ("%d\n%d\n", numeros[i], cubos [i]);
   }

return 0;

}
